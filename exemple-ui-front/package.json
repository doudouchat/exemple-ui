{
  "name": "exemple-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --aot=false",
    "test": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "test:dev": "ng test --watch --code-coverage --progress --browsers=ChromeHeadless",
    "lint": "npx ng lint exemple-ui",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --watch=false --headless=false --configuration=dev",
    "e2e:docker": "ng e2e --configuration=docker",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.1",
    "@angular/common": "^17.0.1",
    "@angular/compiler": "^17.0.1",
    "@angular/core": "^17.0.1",
    "@angular/forms": "^17.0.6",
    "@angular/localize": "^17.0.6",
    "@angular/platform-browser": "^17.0.1",
    "@angular/platform-browser-dynamic": "^17.0.6",
    "@angular/router": "^17.0.6",
    "@ngxs/router-plugin": "^3.8.2",
    "@ngxs/storage-plugin": "^3.8.2",
    "@ngxs/store": "^3.8.2",
    "crypto-js": "^4.2.0",
    "fast-json-patch": "^3.1.1",
    "moment": "^2.29.4",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular-eslint/builder": "^17.1.1",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.1.1",
    "@angular-eslint/template-parser": "^17.1.1",
    "@angular/cli": "^17.0.6",
    "@angular/compiler-cli": "^17.0.1",
    "@angular/language-service": "^17.0.6",
    "@badeball/cypress-cucumber-preprocessor": "^19.2.0",
    "@cypress/schematic": "^2.5.1",
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@sinonjs/fake-timers": "^11.2.2",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.3",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cassandra-driver": "^4.7.2",
    "chai": "^4.3.10",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^10.2.0",
    "process": "^0.11.10",
    "sinon": "^17.0.1",
    "sonarqube-scanner": "^3.3.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "util": "^0.12.5"
