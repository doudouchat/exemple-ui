{
  "name": "exemple-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --aot=false",
    "test": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "test:dev": "ng test --watch --code-coverage --progress --browsers=ChromeHeadless",
    "lint": "ng lint exemple-ui",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --watch=false --headless=false --configuration=dev",
    "e2e:docker": "ng e2e --configuration=docker",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/localize": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@ngxs/router-plugin": "^20.1.0",
    "@ngxs/storage-plugin": "^20.1.0",
    "@ngxs/store": "^20.1.0",
    "@primeng/themes": "^20.3.0",
    "crypto-js": "^4.2.0",
    "fast-json-patch": "^3.1.1",
    "moment": "^2.30.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/language-service": "^21.0.0",
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.7",
    "@cypress/schematic": "^4.1.4",
    "@sinonjs/fake-timers": "^15.0.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.1",
    "@types/sinon": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "angular-eslint": "^21.0.0",
    "cassandra-driver": "^4.8.0",
    "chai": "^6.2.1",
    "cypress": "^15.6.0",
    "eslint": "^9.39.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^11.7.5",
    "ng-mocks": "^14.14.0",
    "sinon": "^21.0.0",
    "sonarqube-scanner": "^4.3.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "e2e/steps/*.ts"
    ]
  }
}
