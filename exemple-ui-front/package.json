{
  "name": "exemple-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --aot=false",
    "test": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "test:dev": "ng test --watch --code-coverage --progress --browsers=ChromeHeadless",
    "lint": "ng lint exemple-ui",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --watch=false --headless=false --configuration=dev",
    "e2e:docker": "ng e2e --configuration=docker",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.1",
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.2.13",
    "@angular/localize": "^18.2.13",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@ngxs/router-plugin": "^18.1.6",
    "@ngxs/storage-plugin": "^18.1.6",
    "@ngxs/store": "^18.1.6",
    "crypto-js": "^4.2.0",
    "fast-json-patch": "^3.1.1",
    "moment": "^2.30.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.12",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.0.1",
    "@angular/language-service": "^18.2.13",
    "@badeball/cypress-cucumber-preprocessor": "^21.0.3",
    "@cypress/schematic": "^2.5.2",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@sinonjs/fake-timers": "^13.0.5",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.9",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "angular-eslint": "^18.4.3",
    "cassandra-driver": "^4.7.2",
    "chai": "^5.1.2",
    "cypress": "^13.16.1",
    "eslint": "^9.16.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^11.0.1",
    "ng-mocks": "^14.13.1",
    "sinon": "^19.0.2",
    "sonarqube-scanner": "^4.2.6",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.17.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "e2e/steps/*.ts"
    ]
  }
}
