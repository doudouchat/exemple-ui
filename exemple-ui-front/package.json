{
  "name": "exemple-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --aot=false",
    "test": "ng test --no-watch --code-coverage",
    "test:dev": "ng test --watch --code-coverage --progress --browsers=ChromeHeadless",
    "lint": "ng lint exemple-ui",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --watch=false --headless=false --configuration=dev",
    "e2e:docker": "ng e2e --configuration=docker",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.0",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.1",
    "@angular/localize": "^20.1.1",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.1.1",
    "@angular/router": "^20.1.1",
    "@ngxs/router-plugin": "^20.1.0",
    "@ngxs/storage-plugin": "^20.1.0",
    "@ngxs/store": "^20.1.0",
    "@primeng/themes": "^20.0.0",
    "crypto-js": "^4.2.0",
    "fast-json-patch": "^3.1.1",
    "moment": "^2.30.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/language-service": "^20.1.1",
    "@badeball/cypress-cucumber-preprocessor": "^22.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "@cypress/schematic": "^3.0.0",
    "@sinonjs/fake-timers": "^14.0.0",
    "@types/node": "^22.16.4",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "angular-eslint": "^20.1.1",
    "cassandra-driver": "^4.8.0",
    "cypress": "^14.5.2",
    "eslint": "^9.31.0",
    "jsdom": "^27.2.0",
    "ng-mocks": "^14.13.5",
    "playwright": "^1.56.1",
    "sinon": "^21.0.0",
    "sonarqube-scanner": "^4.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.1.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "e2e/steps/*.ts"
    ]
  }
}
