{
  "name": "exemple-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --aot=false",
    "test": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "test:dev": "ng test --watch --code-coverage --progress --browsers=ChromeHeadless",
    "lint": "ng lint exemple-ui",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --watch=false --headless=false --configuration=dev",
    "e2e:docker": "ng e2e --configuration=docker",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/localize": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@ngxs/router-plugin": "^19.0.0",
    "@ngxs/storage-plugin": "^19.0.0",
    "@ngxs/store": "^19.0.0",
    "@primeng/themes": "^19.0.0",
    "crypto-js": "^4.2.0",
    "fast-json-patch": "^3.1.1",
    "moment": "^2.30.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/language-service": "^19.0.0",
    "@badeball/cypress-cucumber-preprocessor": "^22.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cypress/schematic": "^3.0.0",
    "@sinonjs/fake-timers": "^14.0.0",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.9",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "angular-eslint": "^19.0.0",
    "cassandra-driver": "^4.7.2",
    "chai": "^5.1.2",
    "cypress": "^14.2.0",
    "eslint": "^9.18.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^11.1.0",
    "ng-mocks": "^14.13.2",
    "sinon": "^19.0.2",
    "sonarqube-scanner": "^4.2.6",
    "ts-loader": "^9.5.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.21.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "e2e/steps/*.ts"
    ]
  }
}
